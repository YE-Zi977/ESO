.my-article.default {
  position: relative;
}
.my-article.default .g-left-box {
  margin-right: 15px;
  color: #999999;
}
.my-article.default .g-right-box {
  font-size: 18px;
  color: #999999;
}
.my-article.default .tab-bar {
  position: relative;
  height: 55px;
  width: 100%;
  border-bottom: 1px solid #dddddd;
  background-color: #ffffff;
}
.my-article.default .tab-bar .line {
  position: absolute;
  bottom: -1px;
  width: 25%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  height: 2px;
  background-color: #fb655d;
  -webkit-transition: left 0.23s linear;
  transition: left 0.23s linear;
}
.my-article.default .tab-bar .item {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  height: 100%;
  font-size: 14px;
  color: #999999;
}
.my-article.default .tab-bar .item.active {
  color: #fb655d;
}
.my-article.default .cont {
  background-color: #ffffff;
}
.my-article.default .cont .head {
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding: 15px 12px;
}
.my-article.default .cont .head .count {
  font-size: 14px;
  font-weight: 600;
  color: #333333;
}
.my-article.default .cont .head .type {
  height: 30px;
  width: 90px;
  -webkit-border-radius: 30rpx;
          border-radius: 30rpx;
  background-color: #f5f5f5;
}
.my-article.default .cont .head .type text {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  height: 100%;
  -webkit-border-radius: 30px;
          border-radius: 30px;
  border: 1px solid transparent;
  font-size: 12px;
  text-align: center;
  line-height: 28px;
  color: #999999;
}
.my-article.default .cont .head .type text.active {
  border-color: #dddddd;
  color: #333333;
  background-color: #ffffff;
}
.my-article.default .cont .list {
  padding: 0 12px 15px;
}
.my-article.default .cont .list .item {
  margin-top: 15px;
  border-bottom: 1px solid #dddddd;
}
.my-article.default .cont .list .item .info {
  font-size: 12px;
  color: #999999;
}
.my-article.default .cont .list .item .info .status {
  margin-right: 5px;
  color: #333333;
}
.my-article.default .cont .list .item .title {
  padding-top: 5px;
  font-size: 18px;
  color: #333333;
}
.my-article.default .cont .list .item .desc {
  min-height: 40px;
  margin: 10px 0;
  font-size: 14px;
  line-height: 20px;
  color: #999999;
}
.my-article.default .no-data {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.my-article.default .no-data image {
  max-width: 100%;
}
.my-article.default .no-data .tip {
  padding-bottom: 10px;
  font-size: 13px;
  color: #999999;
}
.my-article.default .no-data .create-btn {
  height: 40px;
  padding: 0 10px;
  -webkit-border-radius: 40px;
          border-radius: 40px;
  font-size: 14px;
  color: #ffffff;
  background-color: #fb655d;
}
.my-article.default .g-create-btn {
  position: absolute;
  bottom: 100px;
  right: 20px;
  height: 55px;
  width: 55px;
  -webkit-border-radius: 50%;
          border-radius: 50%;
  -webkit-box-shadow: 0 0 8px 2px rgba(234, 116, 86, 0.35);
          box-shadow: 0 0 8px 2px rgba(234, 116, 86, 0.35);
  background-image: -webkit-linear-gradient(315deg, #ef9272, #fb655d);
  background-image: linear-gradient(135deg, #ef9272, #fb655d);
}
.my-article.default .g-create-btn .icon {
  font-size: 26px;
  color: #ffffff;
}
